{
  "name": "nextjs-api-starter",
  "version": "1.0.0",
  "description": "Build a NodeJS Backend and APIs with NextJS, typescript, mongoose, and jest support.",
  "author": "Fenando López Martínez",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "commit": "git-cz",
    "dev": "next",
    "env:pull": "vercel env pull .env.local",
    "execute": "ts-node-dev --respawn --no-notify --transpileOnly --project ./tsconfig.node.json",
    "execute:inspect": "ts-node-dev --inspect-brk --respawn --no-notify --transpileOnly --project ./tsconfig.node.json",
    "lint": "eslint . --ext .js,.ts",
    "semantic-release": "semantic-release",
    "start": "next start",
    "test": "jest --passWithNoTests --runInBand",
    "test:local": "env-cmd -f ./.env.test yarn run test",
    "test:watch": "yarn run test:local --watch",
    "test:coverage": "yarn run test:local --coverage"
  },
  "dependencies": {
    "cors": "2.8.5",
    "mongodb": "3.5.9",
    "mongoose": "5.9.25",
    "next": "9.4.4",
    "pino": "6.4.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "validator": "13.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/cors": "2.8.6",
    "@types/jest": "25.2.3",
    "@types/mongodb": "3.5.25",
    "@types/mongoose": "5.7.32",
    "@types/node": "14.0.5",
    "@types/pino": "6.3.0",
    "@types/react": "16.9.35",
    "@types/validator": "13.1.0",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "env-cmd": "10.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.6",
    "node-mocks-http": "1.8.1",
    "prettier": "2.0.5",
    "semantic-release": "17.2.3",
    "ts-jest": "26.0.0",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.9.3",
    "vercel": "19.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  }
}
